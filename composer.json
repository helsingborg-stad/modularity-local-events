{
    "name": "helsingborg-stad/modularity-local-events",
    "description": "Local events",
    "type": "wordpress-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Eric Rosenborg",
            "email": "eric.rosenborg@helsingborg.se"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-paths": {
            "vendor/{$vendor}/{$name}": [
                "type:wordpress-muplugin",
                "type:wordpress-theme",
                "type:wordpress-plugin",
                "type:acf-plugin"
            ]
        }
    },
    "require": {
        "helsingborg-stad/municipio": "^6.0.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/helsingborg-stad/modularity-local-events.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "ModularityLocalEvents\\": "source/php/"
        }
    },
    "version": "4.0.0"
}
